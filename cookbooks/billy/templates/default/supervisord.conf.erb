[unix_http_server]
file=/home/<%= node[:billy][:user] %>/logs/supervisor.sock
 
[supervisord]
logfile=/home/<%= node[:billy][:user] %>/logs/supervisord.log ;
logfile_maxbytes=50MB ;
logfile_backups=10 ;
loglevel=info ;
pidfile=/home/<%= node[:billy][:user] %>/logs/supervisord.pid ;
nodaemon=false ;
minfds=1024 ;
minprocs=200 ;

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix:///home/<%= node[:billy][:user] %>/logs/supervisor.sock ;
 
[include]
files =
    <%= node[:billy][:install_dir] %>/*.sd.conf
